<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">新規プロトタイプ投稿</h2>
        <%# 部分テンプレートでフォームを表示する %>
      <%= form_with model: @prototype, url: prototypes_path, local: true, multipart: true do |f| %>
        <div class="field">
          <%= f.label :title, "プロトタイプの名称" %><br />
          <%= f.text_field :title, id: "prototype_title", autofocus: true %>
        </div>
        <div class="field">
          <%= f.label :catch_copy, "キャッチコピー" %><br />
          <%= f.text_area :catch_copy, id: "prototype_catch_copy" %>
        </div>
        <div class="field">
          <%= f.label :concept, "コンセプト" %><br />
          <%= f.text_area :concept, id: "prototype_concept" %>
        </div>
        <div class="field">
          <%= f.label :image, "プロトタイプの画像" %><br />
          <%= f.file_field :image, id: "prototype_image" %>
        </div>
        <div class="actions">
          <%= f.submit "投稿する" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
